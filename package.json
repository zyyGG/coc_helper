{
  "name": "coc_helper",
  "version": "1.0.0",
  "description": "在线COC_跑团辅助工具",
  "scripts": {
    "dev": "concurrently 'npm:dev:client' 'npm:dev:server'",
    "build": "concurrently 'npm:build:client' 'npm:build:server'",
    "serve":"pm2 ./server/build/index.js",
    "dev:client": "cd ./client && npm run dev",
    "dev:server": "cd ./server && npm run dev:ts-node",
    "build:client": "cd ./client && npm run build",
    "build:server": "cd ./server && npm run build",
    "auto-install": "npm install && cd client && npm install && cd ../ && cd server && npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.0"
  }
}